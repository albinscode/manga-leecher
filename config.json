{
    "pagePattern": "${pageNumber}",
    "filePattern": "${manga}_${volume}_${pageNumber}.png",
    "urlPattern": "https://www.japscan.co/lecture-en-ligne/${manga}/${volume}/${pagePattern}.html",
    "mode": "dom",
    "blacklist": [
        "syndicate",
        "adtrue",
        "ie8eamus",
        "passeura",
        "google"
    ],
    "maxPageSelector": "#pages",
    "dom": {
        "selector": "cnv-vv",
        "padding": 0
    },
    "screenshot": {
        "coords": {
            "x": 200,
            "y": 300,
            "width": 1170,
            "height": 840
        }
    },
    "debug": false,
    "viewPort": {
        "width": 3200,
        "height": 2400,
        "deviceScaleFactor": 1
    }
}
